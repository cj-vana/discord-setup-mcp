{
  "name": "discord-setup-mcp",
  "version": "2.0.0",
  "description": "MCP server for automating Discord server setup using Discord Bot API - cross-platform, fast, and reliable",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "discord-setup-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
